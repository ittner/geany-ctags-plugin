	    echo " $(LIBTOOL) --mode=install $(libgeanyluaLTLIBRARIES_INSTALL) $(INSTALL_STRIP_FLAG) '$$p' '$(DESTDIR)$(libgeanyluadir)/$$f'"; \
	    $(LIBTOOL) --mode=install $(libgeanyluaLTLIBRARIES_INSTALL) $(INSTALL_STRIP_FLAG) "$$p" "$(DESTDIR)$(libgeanyluadir)/$$f"; \
	  else :; fi; \
	done

